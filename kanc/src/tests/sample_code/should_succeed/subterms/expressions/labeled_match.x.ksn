match x {
    a => b,
    b(x') => c,
    c(:x') => x',
    d(:x', :y') => y',
    e(foo: x') => x',
    f(foo: x', bar: y') => y',
    g(:x', bar: y') => x',
    h(foo: x', :y') => x',
    i(foo: _) => x,
    j(foo: _, bar: _) => y,
    k(:z, foo: _) => x,
    l(foo: _, :z) => z,
    m(...) => x,
    n(:z, ...) => z,
    o(foo: q, :bar, ...) => q,
}
